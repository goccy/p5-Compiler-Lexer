subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'args' => '',
            'name' => 'warnings'
          },
          {
            'args' => '  ( )',
            'name' => 'mypragma'
          },
          {
            'args' => '',
            'name' => 'mypragma'
          },
          {
            'args' => '',
            'name' => 'Sans_mypragma'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

done_testing;
