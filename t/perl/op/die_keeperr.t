          },
          {
            'token_num' => 5,
            'has_warnings' => 1,
            'end_line' => 6,
            'src' => ' plan ( 20 ) ;',
            'start_line' => 6,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'token_num' => 28,
            'has_warnings' => 1,
            'end_line' => 14,
            'src' => ' sub end ( & ) { my ( $c ) = @_ ; return bless ( sub { $c-> ( ) } , "End" ) ; }',
            'start_line' => 11,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 7,
