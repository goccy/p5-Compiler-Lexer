    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'args' => '',
            'name' => 'warnings'
          },
          {
            'args' => '',
            'name' => 'Tie::Array'
          },
          {
            'args' => '',
            'name' => 'Tie::Hash'
          },
          {
            'args' => '  \'""\'  =>  sub  { "object" }  ,  fallback  =>  1',
            'name' => 'overload'
          },
          {
            'args' => '  \'~~\'  =>  sub  { my %hash = %{ $_ [ 0 ] } ; if ( $_ [ 2 ] ) { return $_ [ 1 ] eq reverse $hash { key } ; } else { return $_ [ 1 ] eq $hash { key } ; } }',
            'name' => 'overload'
          },
          {
            'args' => '  \'""\'  =>  sub  { "stringified" }',
            'name' => 'overload'
          },
          {
