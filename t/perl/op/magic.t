subtest 'get_groups_by_syntax_level' => sub {
    my $lexer = Compiler::Lexer->new('');
    my $tokens = $lexer->tokenize($script);
    my $stmts = $lexer->get_groups_by_syntax_level($tokens, Compiler::Lexer::SyntaxType::T_Stmt);
    is_deeply($stmts, [
          {
            'src' => ' $| = 1 ;',
            'token_num' => 4,
            'start_line' => 4,
            'block_id' => 1,
            'indent' => 1,
            'has_warnings' => 0,
            'end_line' => 4
          },
          {
            'end_line' => 5,
            'has_warnings' => 0,
            'start_line' => 5,
            'indent' => 1,
            'block_id' => 1,
            'token_num' => 6,
            'src' => ' chdir \'t\' if -d \'t\' ;'
          },
          {
            'has_warnings' => 0,
            'end_line' => 6,
            'token_num' => 4,
            'src' => ' @INC = \'../lib\' ;',
            'start_line' => 6,
            'indent' => 1,
            'block_id' => 1
          },
          {
            'block_id' => 1,
            'start_line' => 7,
            'indent' => 1,
            'token_num' => 3,
            'src' => ' require \'./test.pl\' ;',
            'end_line' => 7,
            'has_warnings' => 0
          },
          {
            'src' => ' plan ( tests => 192 ) ;',
            'token_num' => 7,
            'block_id' => 1,
            'start_line' => 8,
            'indent' => 1,
            'has_warnings' => 1,
            'end_line' => 8
          },
          {
            'block_id' => 2,
            'start_line' => 18,
            'indent' => 1,
            'src' => ' my %non_mini = map { $_ => 1 } qw(+ - [) ;',
            'token_num' => 14,
            'end_line' => 18,
            'has_warnings' => 0
          },
          {
            'has_warnings' => 1,
            'end_line' => 32,
            'token_num' => 50,
            'src' => ' for ( qw(
	SIG ^OPEN ^TAINT ^UNICODE ^UTF8LOCALE ^WARNING_BITS 1 2 3 4 5 6 7 8
	9 42 & ` \' : ? ! _ - [ ^ ~ = % . ( ) < > \\/ $ | + ; ] ^A ^C ^D
	^E ^F ^H ^I ^L ^N ^O ^P ^S ^T ^V ^W ^UTF8CACHE ::12345 main::98732
	^LAST_FH
    )) {
	my $v = $_;
	# avoid using any global vars here:
	if ($v =~ s/ ^ \\ ^ ( ? = . ) // ) { for ( substr $v , 0 , 1 ) { $_ = chr ( utf8::native_to_unicode ( ord ( $_ ) ) - 64 ) ; } }',
            'start_line' => 19,
            'indent' => 1,
            'block_id' => 2
          },
          {
            'src' => ' for ( substr $v , 0 , 1 ) { $_ = chr ( utf8::native_to_unicode ( ord ( $_ ) ) - 64 ) ; }',
            'token_num' => 26,
            'indent' => 2,
            'start_line' => 29,
            'block_id' => 3,
            'has_warnings' => 1,
            'end_line' => 31
          },
          {
            'end_line' => 30,
            'has_warnings' => 1,
            'indent' => 3,
            'start_line' => 30,
            'block_id' => 4,
            'token_num' => 15,
            'src' => ' $_ = chr ( utf8::native_to_unicode ( ord ( $_ ) ) - 64 ) ;'
          },
          {
            'start_line' => 35,
            'block_id' => 5,
            'indent' => 2,
            'token_num' => 14,
            'src' => ' skip_if_miniperl ( "the module for *$_ may not be available in " . "miniperl" , 1 ) if $non_mini { $_ } ;',
            'end_line' => 36,
            'has_warnings' => 1
          },
          {
            'indent' => 2,
            'start_line' => 37,
            'block_id' => 5,
            'token_num' => 7,
            'src' => ' ok defined * $v , "*$_ appears to be defined at the outset" ;',
            'end_line' => 37,
            'has_warnings' => 1
          }
        ]
, 'Compiler::Lexer::get_groups_by_syntax_level');
};

