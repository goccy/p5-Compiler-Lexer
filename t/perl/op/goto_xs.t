            'src' => ' use vars \'$VALID\' ;',
            'start_line' => 21,
            'indent' => 0,
            'block_id' => 0
          },
          {
            'token_num' => 7,
            'has_warnings' => 1,
            'end_line' => 23,
            'src' => ' plan ( tests => 11 ) ;',
            'start_line' => 23,
