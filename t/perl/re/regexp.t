subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'args' => '  FATAL  =>  "all"',
            'name' => 'warnings'
          },
          {
            'args' => '  qw ( $bang $ffff $nulnul )',
            'name' => 'vars'
          },
          {
            'args' => '  qw ( $qr $skip_amp $qr_embed $qr_embed_thr )',
            'name' => 'vars'
          },
          {
            'args' => '  }',
            'name' => 'Data::Dumper'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

