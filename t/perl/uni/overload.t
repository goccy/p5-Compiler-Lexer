subtest 'get_used_modules' => sub {
    my $modules = Compiler::Lexer->new('')->get_used_modules($script);
    is_deeply($modules, [
          {
            'args' => '',
            'name' => 'strict'
          },
          {
            'args' => '  \'""\'  =>  \'stringify\'  ,  fallback  =>  1',
            'name' => 'overload'
          },
          {
            'args' => '',
            'name' => 'locale'
          },
          {
            'args' => '  \'""\'  =>  sub  { $_ [ 0 ]-> { data } }  }',
            'name' => 'overload'
          }
        ]
, 'Compiler::Lexer::get_used_modules');
};

